		};

		dma7: dma@7 {
			compatible = "adi,dma2";
			reg = <0x31022380 0x7F>;
			interrupts = <0 96 0>;
			spu_securep_id = <55>;
		};

		dma8: dma@8 {
			compatible = "adi,dma2";
			reg = <0x310A7000 0x7F>;
			interrupts = <0 150 0>;
			spu_securep_id = <102>;
		};

		dma9: dma@9 {
			compatible = "adi,dma2";
