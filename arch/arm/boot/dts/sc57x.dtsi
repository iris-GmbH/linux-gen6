		};

		dma26: dma@26 {
			compatible = "adi,dma2";
			reg = <0x31046200 0x7F>;
			interrupts = <0 68 0>;
			spu_securep_id = <73>;
		};

		dma27: dma@27 {
			compatible = "adi,dma2";
			reg = <0x31046280 0x7F>;
			interrupts = <0 69 0>;
			spu_securep_id = <74>;
		};

		dma28: dma@28 {
			compatible = "adi,dma2";
			reg = <0x3102B300 0x7F>;
			interrupts = <0 81 0>;
			spu_securep_id = <66>;
		};

		dma29: dma@29 {
			compatible = "adi,dma2";
			reg = <0x3102B380 0x7F>;
			interrupts = <0 82 0>;
			spu_securep_id = <67>;
		};

		dma30: dma@30 {
			compatible = "adi,dma2";
			reg = <0x30FFF000 0x7F>;
			interrupts = <0 77 0>;
			spu_securep_id = <4>;
		};

		dma34: dma@34 {
			compatible = "adi,dma2";
			reg = <0x31026180 0x7F>;
			interrupts = <0 109 0>;
			spu_securep_id = <59>;
		};

		dma35: dma@35 {
			compatible = "adi,dma2";
			reg = <0x31026100 0x7F>;
			interrupts = <0 110 0>;
			spu_securep_id = <58>;
		};

		dma36: dma@36 {
			compatible = "adi,dma2";
			reg = <0x30FFF080 0x7F>;
			interrupts = <0 79 0>;
			spu_securep_id = <5>;
		};

		dma37: dma@37 {
			compatible = "adi,dma2";
			reg = <0x31026280 0x7F>;
			interrupts = <0 112 0>;
			spu_securep_id = <61>;
		};

		dma38: dma@38 {
			compatible = "adi,dma2";
			reg = <0x31026200 0x7F>;
			interrupts = <0 113 0>;
			spu_securep_id = <60>;
