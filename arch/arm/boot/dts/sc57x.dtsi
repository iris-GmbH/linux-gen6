		dma21: dma@21 {
			compatible = "adi,dma2";
			reg = <0x31026000 0x7F>;
			interrupts = <0 107 0>;
			spu_securep_id = <56>;
		};

		dma22: dma@22 {
			compatible = "adi,dma2";
			reg = <0x3102B000 0x7F>;
			interrupts = <0 98 0>;
			spu_securep_id = <62>;
		};

		dma23: dma@23 {
			compatible = "adi,dma2";
			reg = <0x3102B080 0x7F>;
			interrupts = <0 99 0>;
			spu_securep_id = <63>;
		};

		dma24: dma@24 {
			compatible = "adi,dma2";
			reg = <0x3102B100 0x7F>;
			interrupts = <0 102 0>;
			spu_securep_id = <64>;
